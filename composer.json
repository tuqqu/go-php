{
    "name": "tuqqu/go-php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur Kurbidaev",
            "email": "artkurbidaev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "tuqqu/go-parser": "^0.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.0",
        "friendsofphp/php-cs-fixer": "^3.8",
        "phpunit/phpunit": "^9",
        "vimeo/psalm": "5.0.0-beta1"
    },
    "bin": [
        "bin/go-php"
    ],
    "autoload": {
        "psr-4": {
            "GoPhp\\": "src/"
        },
        "files": [
            "src/asserts.php",
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "GoPhp\\Tests\\": "src/"
        }
    }
}
